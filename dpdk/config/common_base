#
# String that appears before the version number
#
CONFIG_RTE_VER_PREFIX="DPDK"

#
# Version information completed when this file is processed for a build
#
CONFIG_RTE_VER_YEAR=__YEAR
CONFIG_RTE_VER_MONTH=__MONTH
